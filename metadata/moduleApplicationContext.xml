				<prop key="/module/@MODULE_ID@/configurationManager.form">configurationManager</prop>
				<prop key="/module/@MODULE_ID@/configurationManagerSuccess.form">configurationManagerSuccess</prop>
				<prop key="/module/@MODULE_ID@/displayBadScans.form">displayBadScans</prop>
				<prop key="/module/@MODULE_ID@/faxableForm.form">faxableForm</prop>
				<prop key="/module/@MODULE_ID@/createClinicForm.form">createClinicForm</prop>
				<prop key="/module/@MODULE_ID@/createClinicTagForm.form">createClinicTagForm</prop>
			    <prop key="/module/@MODULE_ID@/importConceptsFromFile.form">importConceptsFromFile</prop>
			    <prop key="/module/@MODULE_ID@/createFormFromFile.form">createFormFromFile</prop>
			</props>
		</property>
	</bean>
<bean id="ruleBuilderForm" class="org.openmrs.module.atd.web.RuleBuilderFormController" lazy-init="default" autowire="default" dependency-check="default">
<property name="commandName"><value>rule</value></property>
<property name="commandClass">
  <value>org.openmrs.module.atd.RuleBuilderForm</value> 
  </property>
<property name="formView"><value>/module/@MODULE_ID@/ruleBuilderForm</value></property>
<property name="successView"><value>ruleBuilder.form</value></property>
  </bean>
  
