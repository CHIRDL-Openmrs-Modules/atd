<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.atd">

	<class name="org.openmrs.module.atd.hibernateBeans.Session" table="atd_session">

		<id name="sessionId" type="java.lang.Integer" column="session_id">
			<generator class="native" />
		</id>
			<property name="encounterId" type="java.lang.Integer" column="encounter_id"/>
	</class>
	
</hibernate-mapping>
